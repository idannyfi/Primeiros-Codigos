from random import shuffle
n1 = str(input('Digite o primeiro nome: ')).strip().title()
n2 = str(input('Digite o segundo nome: ')).strip().title()
n3 = str(input('Digite o terceiro nome: ')).strip().title()
n4 = str(input('Digite o quarto nome: ')).strip().title()

lista = [n1, n2, n3 ,n4]
nova_lista = shuffle(lista)
print('-'*70)
print(f'A nova ordem ser√° :{lista}')
print('-'*70)


